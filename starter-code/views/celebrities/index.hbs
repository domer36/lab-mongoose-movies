<h2>Celebrities</h2>
{{#if error}} <h2>No found celebrities</h2> {{/if}}
<a href="/celebrities/new">Add Celebrity</a>
{{#each celebrities}}
<li>
  <a href="/celebrities/{{this._id}}">{{ this.name }}</a> | 
  <a href="/celebrities/{{this._id}}/delete">Delete</a> | 
  <a href="/celebrities/{{this._id}}/edit">Edit</a>
</li>
{{/each}}